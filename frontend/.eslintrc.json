{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react" 
    ],
    "rules": {
        // either add all prop types to get rid of the warning, or keep it off for MVP
        "react/prop-types": "off" 
    }
}
